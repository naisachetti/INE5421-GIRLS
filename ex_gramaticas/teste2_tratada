Z ::= z | d Z' | c Z''
S ::= d S' | c S''
A ::= a A'
B ::= d B' | c B''
C ::= b C'
D ::= d | c S c
Z' ::= A | & | C Z'''
Z'' ::= S Z''''
S' ::= A | & | C Z'''
S'' ::= S S''''
A' ::= B Z'''
B' ::= C | &
B'' ::= S B'''
C' ::= D B'
Z''' ::= A | &
Z'''' ::= c Z'''''
S'''' ::= c Z'''''
B''' ::= c B'
Z''''' ::= A | & | C Z'''
