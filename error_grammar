A ::= & | b A'1 | c A'2 | f A'3 | v A'4 | com ; A'6
A'1 ::= C e | e | V A'5
A'2 ::= & | C | V A'6 | c K A'4
A'3 ::= ; A'7 | P ; A'7
A'4 ::= & | C | V A'6
A'5 ::= C e | e
A'6 ::= & | C
A'7 ::= & | C | f V'1 A'6
C ::= b A'1 | com ; A'6
K ::= & | c K
P ::= b A'1 | c A'2 | f A'3 | v A'4 | com ; A'6
V ::= f V'1 | v V'2
V'1 ::= ; V'3 | P ; V'3
V'2 ::= & | V
V'3 ::= & | f V'1
